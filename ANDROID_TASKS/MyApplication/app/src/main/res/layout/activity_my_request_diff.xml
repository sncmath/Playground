<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ScrollView01"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@drawable/border">

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:background="@drawable/menu_border"
            android:gravity="center_vertical"
            android:orientation="vertical">

            <ImageView
                android:layout_width="70dp"
                android:layout_height="52dp"
                android:layout_gravity="left"
                android:contentDescription="@string/todo"
                android:paddingStart="30dp"
                android:paddingLeft="30dp"
                android:src="@drawable/menu" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#fff"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="202dp"
                android:background="@drawable/my_border"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:background="#fff"
                    android:paddingLeft="10sp"
                    android:paddingTop="20sp"
                    android:paddingRight="10sp"
                    android:paddingBottom="20sp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:padding="20sp"
                        android:text="@string/MyRequest"
                        android:textColor="#505050"
                        android:textSize="20sp" />

                    <Button
                        android:layout_width="173dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end"
                        android:background="@drawable/new_request"
                        android:padding="20sp"
                        android:text="@string/NewRequest"
                        android:textColor="#ffffff"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="359dp"
                    android:layout_height="47dp"
                    android:layout_gravity="center"
                    android:background="@drawable/search_border">


                    <EditText
                        android:id="@+id/editText4"
                        android:layout_width="0dp"
                        android:layout_height="39dp"
                        android:layout_weight="1"
                        android:background="@null"
                        android:ems="10"
                        android:inputType="textPersonName"
                        android:paddingLeft="10sp"
                        android:text="@string/search"
                        android:importantForAutofill="no"
                        android:paddingStart="10sp" />

                    <ImageView
                        android:layout_width="30sp"
                        android:layout_height="32dp"
                        android:contentDescription="@string/todo"
                        android:paddingEnd="10sp"
                        android:paddingRight="10dp"
                        android:src="@drawable/search" />
                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="102dp"
                android:background="@drawable/border">

                <LinearLayout
                    android:layout_width="91dp"
                    android:layout_height="65dp"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/filter"
                        android:contentDescription="@string/todo" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="270dp"
                    android:layout_height="71dp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="31dp"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textView6"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="bottom"
                            android:text="@string/filter"
                            android:textSize="15sp"
                            android:textStyle="normal" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="38dp"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textView7"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/filtertext"
                            android:textSize="14sp"
                            android:textStyle="bold" />
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="140dp"
                    android:background="@drawable/border"
                    android:padding="20dp">

                    <LinearLayout
                        android:layout_width="242dp"
                        android:layout_height="107dp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingBottom="10sp"
                            android:text="@string/purchase"
                            android:textColor="#595959"
                            android:textSize="14sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/submitted"
                            android:textColor="#909090"
                            android:textSize="12sp"
                            android:textStyle="normal" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/date"
                            android:textColor="#595959"
                            android:textSize="14sp"
                            android:textStyle="bold" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:layout_gravity="center"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@drawable/yellow_dot" />

                            <LinearLayout
                                android:layout_width="152dp"
                                android:layout_height="38dp"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="230dp"
                                    android:layout_height="match_parent"
                                    android:gravity="center_vertical"
                                    android:paddingStart="15sp"
                                    android:paddingLeft="15sp"
                                    android:text="@string/waiting"
                                    android:textSize="14sp"
                                    android:textStyle="bold" />
                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="145dp"
                        android:layout_height="99dp"
                        android:layout_gravity="center_vertical|end"
                        android:gravity="center_vertical"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:layout_gravity="center|center_vertical"
                            android:contentDescription="@string/todo"
                            android:src="@drawable/noti" />
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="140dp"
                    android:background="@drawable/border"
                    android:padding="20dp">

                    <LinearLayout
                        android:layout_width="242dp"
                        android:layout_height="107dp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingBottom="10sp"
                            android:text="@string/purchase"
                            android:textColor="#595959"
                            android:textSize="14sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/submitted"
                            android:textColor="#909090"
                            android:textSize="12sp"
                            android:textStyle="normal" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/date"
                            android:textColor="#595959"
                            android:textSize="14sp"
                            android:textStyle="bold" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:layout_gravity="center"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@drawable/green_round" />

                            <LinearLayout
                                android:layout_width="152dp"
                                android:layout_height="38dp"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:gravity="center_vertical"
                                    android:paddingLeft="15sp"
                                    android:text="@string/approved"
                                    android:textSize="14sp"
                                    android:textStyle="bold"
                                    android:paddingStart="15sp" />
                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="145dp"
                        android:layout_height="99dp"
                        android:layout_gravity="center_vertical|end"
                        android:gravity="center_vertical"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:layout_gravity="center|center_vertical"
                            android:contentDescription="@string/todo"
                            android:src="@drawable/noti" />
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="140dp"
                    android:background="@drawable/border"
                    android:padding="20dp">

                    <LinearLayout
                        android:layout_width="242dp"
                        android:layout_height="107dp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingBottom="10sp"
                            android:text="@string/purchase"
                            android:textColor="#595959"
                            android:textSize="14sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/submitted"
                            android:textColor="#909090"
                            android:textSize="12sp"
                            android:textStyle="normal" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/date"
                            android:textColor="#595959"
                            android:textSize="14sp"
                            android:textStyle="bold" />

                        <LinearLayout
                            android:layout_width="235dp"
                            android:layout_height="48dp"
                            android:layout_gravity="center"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@drawable/lemon_dot" />

                            <LinearLayout
                                android:layout_width="152dp"
                                android:layout_height="38dp"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:gravity="center_vertical"
                                    android:paddingStart="15sp"
                                    android:paddingLeft="15sp"
                                    android:text="@string/draft"
                                    android:textSize="14sp"
                                    android:textStyle="bold" />
                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="145dp"
                        android:layout_height="99dp"
                        android:layout_gravity="center_vertical|end"
                        android:gravity="center_vertical"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:layout_gravity="center|center_vertical"
                            android:contentDescription="@string/todo"
                            android:src="@drawable/noti" />
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="140dp"
                    android:background="@drawable/border"
                    android:padding="20dp">

                    <LinearLayout
                        android:layout_width="242dp"
                        android:layout_height="107dp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingBottom="10sp"
                            android:text="@string/purchase"
                            android:textColor="#595959"
                            android:textSize="14sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/submitted"
                            android:textColor="#909090"
                            android:textSize="12sp"
                            android:textStyle="normal" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/date"
                            android:textColor="#595959"
                            android:textSize="14sp"
                            android:textStyle="bold" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:layout_gravity="center"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@drawable/red_dot" />

                            <LinearLayout
                                android:layout_width="152dp"
                                android:layout_height="38dp"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:gravity="center_vertical"
                                    android:paddingStart="15sp"
                                    android:paddingLeft="15sp"
                                    android:text="@string/rejected"
                                    android:textSize="14sp"
                                    android:textStyle="bold" />
                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="145dp"
                        android:layout_height="99dp"
                        android:layout_gravity="center_vertical|end"
                        android:gravity="center_vertical"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:layout_gravity="center|center_vertical"
                            android:contentDescription="@string/todo"
                            android:src="@drawable/noti" />
                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="34dp"
                android:layout_gravity="center_vertical"
                android:background="@drawable/border"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="218dp"
                    android:layout_height="42dp"
                    android:gravity="center_vertical|end"
                    android:text="@string/showmore"
                    android:textSize="15sp" />

                <ImageView
                    android:layout_width="31dp"
                    android:layout_height="46dp"
                    android:contentDescription="@string/todo"
                    android:gravity="start|center_vertical"
                    android:paddingStart="10dp"
                    android:paddingLeft="10dp"
                    android:src="@drawable/down_ero" />


            </LinearLayout>


        </LinearLayout>
    </LinearLayout>
</ScrollView>